//User function Template for Java

class Solution {
    int print2largest(int arr[], int n) {
       int max=-1,max2=-1;
       for(int i=0;i<arr.length;i++){
           if(max<arr[i])max=arr[i];
       }
        for(int i=0;i<arr.length;i++){
           if(max2<arr[i] && arr[i]!=max)max2=arr[i];
       }
       return max2;
    }
}
